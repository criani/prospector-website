  {/* JavaScript for calculations and note toggling */}
  <script>
    const employeesInput = document.getElementById("employees");
    const costDisplay = document.getElementById("costDisplay");
    const calculateBtn = document.getElementById("calculateBtn");

    const toggleEmail = document.getElementById("toggleEmailSecurity");
    const toggleBackup = document.getElementById("toggleUserBackup");
    const toggleServer = document.getElementById("toggleServer");
    const storageNote = document.getElementById("storageNote");

    calculateBtn.addEventListener("click", () => {
      const employees = parseInt(employeesInput.value, 10) || 0;

      // Base price: $18 per user
      let cost = employees * 18;

      // Email protection: +$9 per user
      if (toggleEmail.checked) {
        cost += employees * 9;
      }

      // MS365 backup: +$4 per user
      if (toggleBackup.checked) {
        cost += employees * 4;
      }

      // Server: +$40 flat
      if (toggleServer.checked) {
        cost += 40;
        storageNote.style.display = "block"; // Show the storage note
      } else {
        storageNote.style.display = "none"; // Hide the storage note
      }

      costDisplay.textContent = `$${cost}`;
    });
